global
    maxconn 256
    log stdout local0 

defaults
    mode tcp
    option tcplog
    log global
    timeout connect 5000ms
    timeout client 50000ms
    timeout server 50000ms
    {balance strategy}

frontend ss-in
    bind 127.0.0.1:{port}
    {use_backend strategy}
    default_backend ss-out

backend ss-out
    {backend_default}
